{
  "extends": [
    "@pinwheel/eslint-config"
  ],
  "settings": {
    "react": {
      "version": "16"
    }
  },
  "rules": {
    "space-before-function-paren": 0,
    "react/prop-types": 0,
    "react/jsx-handler-names": 0,
    "react/jsx-fragments": 0,
    "react/no-unused-prop-types": 0,
    "import/export": 0,
    "import/no-extraneous-dependencies": 0,
    "@typescript-eslint/ban-ts-comment": 0,
  }
}
